<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:was="using:WFunUWP.Controls">

    <was:ContainerStyle x:Key="Div" Margin="0" />
    <was:ContainerStyle x:Key="Section" Margin="0" />
    <was:ContainerStyle x:Key="Article" Margin="0" />
    <was:ContainerStyle x:Key="Header" Margin="0" />
    <was:ContainerStyle x:Key="Footer" Margin="0" />
    <was:ContainerStyle x:Key="Main" Margin="0" />
    <was:ContainerStyle x:Key="Figure" Margin="0,12,0,12" />
    <was:ContainerStyle x:Key="Details" Margin="0" />
    <was:ContainerStyle x:Key="Summary" Margin="0" />
    <was:ContainerStyle
        x:Key="Ul"
        Margin="0,12,0,12"
        Padding="12,0,0,0" />
    <was:ContainerStyle x:Key="Ol" Margin="0,12,0,12" />
    <was:ContainerStyle x:Key="Dl" Margin="0,12,0,12" />

    <was:ParagraphStyle x:Key="P" Margin="0,12,0,12" />
    <was:ParagraphStyle
        x:Key="FigCaption"
        Margin="0"
        FontSizeRatio="0.95"
        FontStyle="Italic" />
    <was:ParagraphStyle x:Key="Pre" Margin="0,12,0,12" />
    <was:ParagraphStyle
        x:Key="H1"
        Margin="0,12,0,12"
        FontSizeRatio="1.5"
        FontWeight="SemiBold" />
    <was:ParagraphStyle
        x:Key="H2"
        Margin="0,12,0,12"
        FontSizeRatio="1.25"
        FontWeight="SemiBold" />
    <was:ParagraphStyle
        x:Key="H3"
        Margin="0,6,0,6"
        FontSizeRatio="1.1"
        FontWeight="SemiBold" />
    <was:ParagraphStyle
        x:Key="H4"
        FontSizeRatio="1"
        FontWeight="SemiBold" />
    <was:ParagraphStyle
        x:Key="H5"
        FontSizeRatio="0.90"
        FontWeight="SemiBold" />
    <was:ParagraphStyle
        x:Key="H6"
        FontSizeRatio="0.80"
        FontWeight="SemiBold" />

    <was:ParagraphStyle x:Key="Dt" FontWeight="SemiBold" />
    <was:ParagraphStyle x:Key="Dd" Margin="6,0,0,6" />

    <was:ParagraphStyle
        x:Key="BlockQuote"
        Margin="12"
        FontSizeRatio="1.05"
        FontStyle="Italic" />

    <was:ImageStyle x:Key="Img" Margin="0,12,0,12" />
    <was:ImageStyle x:Key="YouTube" Margin="0,12,0,12" />
    <was:ImageStyle x:Key="Channel9" Margin="0,12,0,12" />

    <was:ListStyle x:Key="Li" Bullet="&#x25CF;" />
    <was:TextStyle x:Key="Span" />
    <was:TextStyle x:Key="Label" />
    <was:TextStyle x:Key="Time" />
    <was:TextStyle x:Key="Strong" FontWeight="SemiBold" />
    <was:TextStyle x:Key="A" Foreground="{ThemeResource SystemAccentColor}" />
    <was:TextStyle x:Key="Code" FontFamily="Courier New" />
    <was:TextStyle x:Key="Q" FontStyle="Italic" />
    <was:TextStyle x:Key="Cite" FontStyle="Oblique" />
    <was:TextStyle x:Key="I" FontStyle="Italic" />
    <was:TextStyle x:Key="Em" FontStyle="Italic" />
    <was:TextStyle x:Key="Mark" FontWeight="SemiBold" />

    <was:TableStyle
        x:Key="Table"
        Margin="0,12,0,12"
        HorizontalAlignment="Center"
        Border="0"
        ColumnWidth="*" />
    <was:ContainerStyle
        x:Key="Td"
        Margin="0"
        Padding="6" />

    <Style TargetType="was:HtmlBlock">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="was:HtmlBlock">
                    <Grid x:Name="_container" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>

        <Setter Property="DefaultDocumentStyle">
            <Setter.Value>
                <was:DocumentStyle
                    A="{StaticResource A}"
                    Article="{StaticResource Article}"
                    BlockQuote="{StaticResource BlockQuote}"
                    Channel9="{StaticResource Channel9}"
                    Cite="{StaticResource Cite}"
                    Code="{StaticResource Code}"
                    Dd="{StaticResource Dd}"
                    Details="{StaticResource Details}"
                    Div="{StaticResource Div}"
                    Dl="{StaticResource Dl}"
                    Dt="{StaticResource Dt}"
                    Em="{StaticResource Em}"
                    FigCaption="{StaticResource FigCaption}"
                    Figure="{StaticResource Figure}"
                    Footer="{StaticResource Footer}"
                    H1="{StaticResource H1}"
                    H2="{StaticResource H2}"
                    H3="{StaticResource H3}"
                    H4="{StaticResource H4}"
                    H5="{StaticResource H5}"
                    H6="{StaticResource H6}"
                    Header="{StaticResource Header}"
                    I="{StaticResource I}"
                    Img="{StaticResource Img}"
                    Label="{StaticResource Label}"
                    Li="{StaticResource Li}"
                    Main="{StaticResource Main}"
                    Mark="{StaticResource Mark}"
                    Ol="{StaticResource Ol}"
                    P="{StaticResource P}"
                    Q="{StaticResource Q}"
                    Section="{StaticResource Section}"
                    Span="{StaticResource Span}"
                    Strong="{StaticResource Strong}"
                    Summary="{StaticResource Summary}"
                    Table="{StaticResource Table}"
                    Td="{StaticResource Td}"
                    Time="{StaticResource Time}"
                    Ul="{StaticResource Ul}"
                    YouTube="{StaticResource YouTube}" />
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
